<div class="row">
   <input class="col-xs-4 col-xs-offset-8 col-sm-4 col-sm-offset-8 col-md-4 col-md-offset-8 col-lg-2 col-lg-offset-10" id="search" type="text" placeholder="Search This Page" onkeypress="searchelements(document.getElementById('search').value)">
</div>
<div class="clearfix"></div>
<script>
function searchelements(searchstring) {
var $rows = jQuery('.name');
$('#search').keyup(function() {
   	var val = $.trim($(this).val()).replace(/ +/g, ' ').toLowerCase();
console.log("The search string is "+val);
   	$rows.show().filter(function() {
       	var text = jQuery(this).text().replace(/\s+/g, ' ').toLowerCase();
       	console.log("The  a search string is "+text);
        if (~text.indexOf(val)){
        	console.log("Element matched. Showing it");
        	jQuery(this).parent().parent().show();
        }
        else{
        	console.log("Element didnt match. Hiding it");
        	jQuery(this).parent().parent().hide();
        }
   	});
});
}
</script>
